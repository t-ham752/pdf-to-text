{
  "name": "go",
  "dockerComposeFile": ["./compose.yaml"],
  "service": "linux",
  "remoteUser": "root",
  "workspaceFolder": "/var/lib/pdf-to-text",
  "customizations": {
    "vscode": {
      "extensions": ["golang.go"],
      "settings": {
        "editor.formatOnSave": true,
        "[go]": {
          "editor.defaultFormatter": "golang.go"
        }
      }
    }
  }
}
